{
  "manifest_version": 3,
  "name": "Clean Up for Xiaohongshu",
  "version": "1.0",
  "description": "Help you clean up the Xiaohongshu",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["https://www.xiaohongshu.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.xiaohongshu.com/*"],
      "js": ["jquery-3.7.1.min.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png"
  }
} 